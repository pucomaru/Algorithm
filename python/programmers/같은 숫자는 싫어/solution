import java.util.*;

public class Main {
    public int[] solution(int[] arr) {
        List<Integer> result = new ArrayList<>();

        int prev = -1 ;

        for (int n : arr) {
            if (prev != n){
                result.add(n);
            }
            prev = n;
        }

        return result.stream().mapToInt(i -> i).toArray();
    }
}