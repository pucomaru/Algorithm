import java.util.*;

public class Main {
    public int solution(int[] nums) {

        int choose = nums.length / 2;
        // 선택할 수 있는 몬스터의 수

        Set<Integer> kinds = new HashSet<>();

        for (int n : nums){
            kinds.add(n);
        }

        return Math.min(kinds.size(), choose);
    }
}